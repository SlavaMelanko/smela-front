{
  "name": "smela-front",
  "private": true,
  "version": "2025.12",
  "type": "module",
  "scripts": {
    "prepare": "husky",
    "clean": "pnpm store prune && rm -rf node_modules pnpm-lock.yaml",
    "reinstall": "pnpm run clean && pnpm install",
    "dev": "vite --mode development",
    "build": "vite build --mode production",
    "build:staging": "vite build --mode staging",
    "bundle:visualize": "ANALYZE_BUNDLE=true vite build --mode production",
    "bundle:analyze": "ANALYZE_BUNDLE=true pnpm run build",
    "preview": "vite preview --mode production",
    "ut": "NODE_ENV=test jest",
    "ut:cov": "NODE_ENV=test jest --coverage",
    "e2e": "pnpm exec playwright test",
    "e2e:ui": "pnpm exec playwright test --ui",
    "tests": "pnpm run ut && pnpm run e2e",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,json,md,yml,yaml,scss}'",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "stylelint": "stylelint 'src/**/*.{scss,css}'",
    "stylelint:fix": "stylelint 'src/**/*.{scss,css}' --fix",
    "stories": "storybook dev -p 6006",
    "stories:build": "storybook build"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --max-warnings=0 --fix",
      "prettier --ignore-unknown --write"
    ],
    "*.{scss,css}": [
      "stylelint --fix"
    ]
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@sentry/react": "^10.27.0",
    "@tanstack/react-pacer": "^0.18.0",
    "@tanstack/react-query": "^5.90.9",
    "@tanstack/react-table": "^8.21.3",
    "clsx": "^2.1.1",
    "http-status-codes": "^2.3.0",
    "i18next": "^25.6.2",
    "i18next-http-backend": "^3.0.2",
    "lucide-react": "^0.553.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-google-recaptcha": "^3.1.0",
    "react-hook-form": "^7.66.0",
    "react-i18next": "^16.3.3",
    "react-multi-select-component": "^4.3.4",
    "react-router-dom": "^7.9.6",
    "react-toastify": "^11.0.5",
    "yup": "^1.7.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-react": "^7.28.5",
    "@eslint/js": "^9.39.1",
    "@hookform/devtools": "^4.4.0",
    "@playwright/test": "^1.56.1",
    "@sentry/vite-plugin": "^4.6.1",
    "@storybook/addon-onboarding": "^10.0.7",
    "@storybook/react-vite": "^10.0.7",
    "@tanstack/eslint-plugin-query": "^5.91.2",
    "@tanstack/react-query-devtools": "^5.90.2",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.4",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react-swc": "^4.2.2",
    "babel-jest": "^30.2.0",
    "eslint": "^9.39.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-storybook": "^10.0.7",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jest-transform-stub": "^2.0.0",
    "lint-staged": "^16.2.6",
    "mailisk": "^2.1.1",
    "prettier": "^3.6.2",
    "prop-types": "^15.8.1",
    "react-test-renderer": "^19.2.0",
    "rollup-plugin-visualizer": "^6.0.5",
    "rollup-plugin-webpack-stats": "^2.1.7",
    "sass": "^1.94.0",
    "sort-json": "^2.0.1",
    "storybook": "^10.0.7",
    "stylelint": "^16.25.0",
    "stylelint-config-recess-order": "^7.4.0",
    "stylelint-config-standard-scss": "^16.0.0",
    "stylelint-scss": "^6.12.1",
    "stylelint-selector-bem-pattern": "^4.0.1",
    "vite": "^7.2.2"
  }
}
