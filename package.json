{
  "name": "smela",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "prepare": "husky",
    "clean": "npm cache clean --force && rm -rf node_modules package-lock.json",
    "reinstall": "npm run clean && npm install",
    "dev": "vite --mode development",
    "build": "vite build --mode production",
    "build:staging": "vite build --mode staging",
    "build:analyze": "ANALYZE=true vite build --mode production && open dist/stats.html",
    "preview": "vite preview --mode production",
    "e2e": "npx playwright test",
    "e2e:ui": "npx playwright test --ui",
    "e2e:ci": "npx playwright test --only-changed",
    "ut": "NODE_ENV=test jest",
    "cov": "NODE_ENV=test jest --coverage",
    "test": "npm run ut && npm run e2e",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,json,md,yml,yaml,scss}'",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "lint:styles": "stylelint 'src/**/*.{scss,css}'",
    "lint:styles:fix": "stylelint 'src/**/*.{scss,css}' --fix",
    "stories": "storybook dev -p 6006 --no-open",
    "stories:build": "storybook build"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --max-warnings=0 --fix",
      "prettier --ignore-unknown --write"
    ],
    "*.{scss,css}": [
      "stylelint --fix"
    ]
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@tanstack/react-query": "^5.90.1",
    "@tanstack/react-table": "^8.21.3",
    "clsx": "^2.1.1",
    "http-status-codes": "^2.3.0",
    "i18next": "^25.5.2",
    "i18next-http-backend": "^3.0.2",
    "lucide-react": "^0.544.0",
    "mailisk": "^2.0.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-google-recaptcha": "^3.1.0",
    "react-hook-form": "^7.55.0",
    "react-i18next": "^15.4.1",
    "react-router-dom": "^7.4.1",
    "react-toastify": "^11.0.5",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@codecov/vite-plugin": "^1.9.1",
    "@eslint/js": "^9.21.0",
    "@hookform/devtools": "^4.4.0",
    "@playwright/test": "^1.52.0",
    "@storybook/addon-onboarding": "^9.1.5",
    "@storybook/react-vite": "^9.1.5",
    "@tanstack/eslint-plugin-query": "^5.83.1",
    "@tanstack/react-query-devtools": "^5.84.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.5.2",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react-swc": "^4.1.0",
    "babel-jest": "^30.1.2",
    "eslint": "^9.21.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-storybook": "^9.1.5",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.1.2",
    "jest-transform-stub": "^2.0.0",
    "lint-staged": "^16.2.0",
    "prettier": "^3.6.2",
    "prop-types": "^15.8.1",
    "react-test-renderer": "^19.1.0",
    "rollup-plugin-visualizer": "^6.0.3",
    "sass": "^1.86.0",
    "sort-json": "^2.0.1",
    "storybook": "^9.1.5",
    "stylelint": "^16.18.0",
    "stylelint-config-recess-order": "^7.3.0",
    "stylelint-config-standard-scss": "^16.0.0",
    "stylelint-scss": "^6.11.1",
    "stylelint-selector-bem-pattern": "^4.0.1",
    "typescript": "^5.9.3",
    "vite": "^7.1.11"
  }
}
